                      <div ng-repeat="lista in listaInput" ng-class="{ 'sc-m-t-lg':$index>0 }">
                        <input type="text" class="sc-input-default" style="width:95%;" ng-model="modeloDefault.instrucoesCobranca" placeholder="Ex: Não receber após vencimento.">
                        <a ng-show="listaInput.length > 1" style="left: .8em; margin: 0;" class="pull-right sc-m-lg" id="fechar">
                          <div class="circleHover"></div>
                          <i class="sc-icon sc-icon-lixeira-1"></i>
                        </a>
                      </div>
                    </label>
                  </div>
                  <div class="sc-m-t-lg sc-m-b-lg col-xs-12 col-sm-12">
                    <label style="width:100%;">
                      <div class="sc-label">Observação Boleto</div>
                      <textarea class="sc-input-default" style="width:100%;" rows="3" ng-model="modeloDefault.observacaoBoleto" placeholder="Ex: Após vencimento juros 2% a.d."></textarea>
                    </label>
                  </div>
                </div>
              </div>
            </form>
          </div>
        </div>

        <div class="sc-modal-footer">
          <button ng-click="salvarModelo()" ng-show="criandoEditandoModelo" class="sc-btn sc-btn-green">Salvar</button>
          <button ng-click="cancelarCriarEditar()" ng-show="criandoEditandoModelo" class="sc-btn sc-btn-yellow">Cancelar</button>
        </div>
      </div>
    </div>
